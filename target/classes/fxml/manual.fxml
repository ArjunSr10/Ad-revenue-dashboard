<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<TilePane alignment="CENTER" fx:id="root" xmlns:fx="http://javafx.com/fxml"
          fx:controller="main.dash.scene.controller.ManualController">
    <ScrollPane>
        <VBox alignment="CENTER" spacing="10">
            <ImageView fx:id="pdfImageView" fitWidth="600" fitHeight="800" />
            <HBox alignment="CENTER" spacing="20">
                <Button text="Previous" fx:id="previousButton" />
                <Button text="Next" fx:id="nextButton" />
            </HBox>
            <Button text="Return" fx:id="returnButton"/>
        </VBox>
    </ScrollPane>
</TilePane>
